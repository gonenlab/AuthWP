{
    "name": "WPMW",
    "version": "0.4.0",
    "author": [
        "Ciaran Gultnieks",
        "Johan Hattne"
    ],
    "description": "Provides WordPress login integration",
    "license-name": "AGPL-3.0-or-later",
    "type": "other",
    "url": "https://www.mediawiki.org/wiki/Extension:WPMW",
    "requires": {
        "MediaWiki": ">= 1.27"
    },
    "AutoloadClasses": {
        "WPMWAuthenticationProvider": "includes/WPMWAuthenticationProvider.php",
        "WPMWSessionProvider": "includes/WPMWSessionProvider.php"
    },
    "ConfigRegistry": {
        "WPMW": "GlobalVarConfig::newInstance"
    },
    "config": {
        "WPMWPath":  "..",
        "WPMWPriority": "100"
    },
    "Hooks": {
        "UserLogout": [ "WPMWSessionProvider::onUserLogout" ]
    },
    "AuthManagerAutoConfig": {
        "primaryauth": {
            "WPMWAuthenticationProvider": {
                "class": "WPMWAuthenticationProvider",
                "args": []
            }
        }
    },
    "SessionProviders": {
        "WPMWSessionProvider": {
            "class": "WPMWSessionProvider",
            "args": []
        }
    },
    "manifest_version": 1
}
